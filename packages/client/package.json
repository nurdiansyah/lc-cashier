{
  "name": "@deboxsoft/lc-cashier-client",
  "version": "1.0.1-beta.9",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/deboxsoft/lc-cashier",
    "directory": "packages/client"
  },
  "maintainers": [
    {
      "name": "Nurdiansyah",
      "email": "nur.diansyah.ckt@gmail.com"
    }
  ],
  "main": "index.js",
  "module": "index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "*",
    "!src",
    "!tests"
  ],
  "scripts": {
    "build": "run-p build:rollup build:tsc",
    "build:rollup": "rollup -c",
    "build:tsc": "rimraf types && tsc",
    "lint": "eslint --fix 'src/**/*.ts'",
    "test": "uvu -r ts-node/register",
    "bump": "pnpm run build && bump",
    "release:dev": "git add . && pnpm run bump -- -cp prerelease && pnpm publish --tag dev --no-git-checks"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@deboxsoft/lc-cashier-api": "workspace:~",
    "@graphql-typed-document-node/core": "^3.1.0"
  },
  "devDependencies": {
    "@deboxsoft/module-client": "~2.2.4",
    "@deboxsoft/module-core": "~2.2.11",
    "@deboxsoft/module-graphql": "~2.2.10",
    "@deboxsoft/plop-generator": "^1.0.1-beta.15",
    "@nurdiansyah/bump": "^6.1.2",
    "@nurdiansyah/rollup": "^1.6.8",
    "ava": "^3.15.0",
    "cross-fetch": "^3.1.4",
    "graphql": "^15.4.0",
    "graphql-subscriptions-client": "^0.16.0",
    "graphql-tag": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "svelte": "^3.31.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "~4.2.0",
    "utility-types": "^3.10.0",
    "uvu": "^0.5.1"
  },
  "peerDependencies": {
    "@deboxsoft/module-client": "*",
    "@deboxsoft/module-core": "*",
    "@deboxsoft/module-graphql": "*",
    "cross-fetch": "*",
    "graphql": "^15.4.0",
    "graphql-subscriptions-client": "*",
    "svelte": "^3.31.0"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "preferUnplugged": true,
  "gitHead": "b023f06cd22d9b5d6279f3930d90bbfc15186ab2"
