    "@nurdiansyah/bump": "^6.1.2",
    "@nurdiansyah/rollup": "^1.6.8",
    "@types/mustache": "^4.1.1",
    "dayjs": "^1.10.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "ts-node": "^9.1.1",
    "ts-to-zod": "^1.2.1",
