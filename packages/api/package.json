    "typescript": "~4.2.0",
    "utility-types": "^3.10.0",
    "uvu": "^0.5.1"
  },
  "peerDependencies": {
    "@deboxsoft/module-core": "*",
    "@deboxsoft/users-api": "*",
    "dayjs": "*"
  },
  "preferUnplugged": true,
